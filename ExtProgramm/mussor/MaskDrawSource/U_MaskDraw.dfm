  object Memo1: TMemo
    Left = 368
    Top = 40
    Width = 265
    Height = 73
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clGreen
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      'With MaskImg do '
      'begin'
      '  canvas.draw(0,0,sourceimg.picture.bitmap);'
      '  picture.bitmap.mask(canvas.pixels[1,1]); '
      'end;')
    ParentFont = False
    TabOrder = 0
  end
  object Memo2: TMemo
    Left = 368
    Top = 152
    Width = 265
    Height = 65
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clGreen
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      'MemImg.canvas.copymode:=cmSrcCopy;'
      'MemImg.canvas.Draw(0,0,maskimg.picture.bitmap);'
      '{cmSrcCopy - Copies the source bitmap to the '
      'canvas.}')
    ParentFont = False
    TabOrder = 1
  end
  object Memo3: TMemo
    Left = 368
    Top = 256
    Width = 265
    Height = 73
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clGreen
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      'MemImg.canvas.copymode:=cmSrcErase;'
      'MemImg.canvas.draw(0,0,SourceImg.picture.bitmap);'
      '{cmSrcErase - Inverts the image on the canvas and '
      'combines the result with the source bitmap by using '
      'the Boolean AND operator.}')
    ParentFont = False
    TabOrder = 2
  end
  object Memo4: TMemo
    Left = 368
    Top = 368
    Width = 265
    Height = 73
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clGreen
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      'DestImg.canvas.copymode:=cmSrcAnd;'
      'DestImg.canvas.draw(10,10,maskimg.picture.bitmap);'
      '{cmSrcAnd - Combines the image on the canvas and '
      'source bitmap by using the Boolean AND operator.}')
    ParentFont = False
    TabOrder = 3
  end
  object Memo5: TMemo
    Left = 368
    Top = 480
    Width = 273
    Height = 65
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clGreen
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      'DestImg.canvas.copymode:=cmSrcInvert;'
      'DestImg.canvas.draw(10,10,memimg.picture.bitmap);'
      '{cmSrcInvert - Combines the image on the canvas and '
      'the source bitmap by using the Boolean XOR operator.}')
    ParentFont = False
    TabOrder = 4
  end
  object Memo6: TMemo
    Left = 24
    Top = 96
    Width = 313
    Height = 449
    Color = clYellow
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clWindowText
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = [fsBold]
    Lines.Strings = (
      'Our objective is to place the balloon in the sky '
      'without copying the gray area that surrounds the '
      'balloon image.'
      ''
      'The Delphi code here is a simpified version of the '
      'code used in its TransparentStretchtBlt function, '
      'whch is called by TBitmap'#39's Draw method when the '
      'Transparent property is True and by TCanvas'#39' s'
      'BrushCopy when BrushStyle is bsClear. '
      ''
      'The idea is to get all zero pixels (black)  on the '
      'balloon bitmap for all areas outside of the balloon '
      'image  and on the destination image wherever the '
      'balloon image is to appear.  We can then  OR the two '
      'images together.  '
      ''
      'Anything ORed with zero remains unchanged so the '
      'balloon image will not change because all of its pixels '
      'match with zero pixels on the destination,  Similarly, '
      'the background will not change because all of it'#39's '
      'pixels have zero pixels in the correponding location of '
      'the balloon image.'
      ''
      'Pretty slick!! '
      ' '
      'Note that Steps 1, 2A and 2B need be performed only '
      'once, so we should be able to beat Delphi'#39's Draw '
      'routines if we save the "Mem" image and perform '
      'steps 3 and 4 ourselves.'
      ''
      'Also, even though Delphi uses cmSrcInvert (XOR, '
      'exclusive or)  to draw the image in Step 4, '
      'cmSrcPaint (OR) copymode would work as well.')
    ParentFont = False
    TabOrder = 5
  end
